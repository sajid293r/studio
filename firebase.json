{
  "hosting": {
    "site": "stayverify",
    "source": ".",
    "rewrites": [
      {
        "source": "**",
        "apphosting": {
          "backendId": "studio",
          "region": "us-central1"
        }
      }
    ]
  },
  "apphosting": [
    {
      "backendId": "stay-verify-perfect",
      "rootDir": "/",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "functions"
      ]
    },
    {
      "backendId": "stay-verify",
      "rootDir": "/",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "functions"
      ]
    }
  ],
  "firestore": {
    "database": "(default)",
    "location": "nam5",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  }
}
